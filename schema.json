{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Tempo Team Schema",
    "type": "array",
    "items": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "isNotEmpty": true
        },
        "roll": {
          "type": "string",
          "isNotEmpty": true
        }        
      },
      "required": ["name", "roll"]
    }
  }
  
